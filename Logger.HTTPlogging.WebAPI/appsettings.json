{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information" //this line should be in appsettings.Development.json for debuggging purpose
    },
    "File": {
      "Path": "Logs/app.log",
      "MaxFileSize": 10485760,
      "MaxFiles": 5
    }
  },
  "AllowedHosts": "*"
}
